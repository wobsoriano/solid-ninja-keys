{
  "name": "solid-ninja-keys",
  "version": "0.1.2",
  "description": "Add cmd+k interface to your solid site.",
  "keywords": [
    "solid",
    "keyboard",
    "shortcut",
    "keypress",
    "hotkey"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-ninja-keys.git"
  },
  "author": "Robert Anthonie Soriano",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf node_modules dist",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "homepage": "https://github.com/wobsoriano/solid-ninja-keys#readme",
  "peerDependencies": {
    "solid-js": "^1.0.0"
  },
  "dependencies": {
    "ninja-keys": "^1.1.12",
    "solid-js": "^1.3.3"
  },
  "devDependencies": {
    "bumpp": "^7.1.1",
    "rollup": "^2.58.0",
    "rollup-preset-solid": "^1.0.1",
    "typescript": "^4.5.3"
  }
}
