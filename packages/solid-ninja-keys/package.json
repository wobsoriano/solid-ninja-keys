{
  "name": "solid-ninja-keys",
  "version": "0.2.0",
  "description": "Add cmd+k interface to your solid site.",
  "keywords": [
    "solid",
    "keyboard",
    "shortcut",
    "keypress",
    "hotkey"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-ninja-keys.git"
  },
  "author": "Robert Anthonie Soriano",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf node_modules dist",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "homepage": "https://github.com/wobsoriano/solid-ninja-keys#readme",
  "peerDependencies": {
    "solid-js": "^1.0.0"
  },
  "dependencies": {
    "ninja-keys": "^1.1.12",
    "solid-js": "^1.3.3"
  },
  "devDependencies": {
    "bumpp": "^7.1.1",
    "esbuild": "^0.14.27",
    "esbuild-plugin-solid": "^0.4.2",
    "tsup": "^5.12.1",
    "typescript": "^4.5.3"
  }
}
